{
  "name": "polymershop",
  "version": "1.0.0",
  "description": "A modern e-commerce application built with Polymer and Material Design",
  "main": "index.html",
  "scripts": {
    "start": "polymer serve",
    "build": "polymer build",
    "test": "polymer test",
    "test:coverage": "polymer test --coverage",
