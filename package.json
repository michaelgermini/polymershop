  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelgermini/polymershop.git"
  },
  "bugs": {
    "url": "https://github.com/michaelgermini/polymershop/issues"
  },
  "homepage": "https://github.com/michaelgermini/polymershop#readme",
  "devDependencies": {
    "polymer-cli": "^1.9.11"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11"
