    "serve:bundled": "polymer serve build/bundled",
    "lint": "polymer lint",
    "analyze": "polymer analyze > analysis.json"
  },
  "keywords": [
    "polymer",
    "e-commerce",
    "web-components",
    "material-design",
    "pwa",
    "shop",
    "retail"
  ],
